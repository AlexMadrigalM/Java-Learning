//More Operators

package net.polykardz;

public class Main {
    public static void main(String[] args) {
        int x = 100;
        int y = 20;

        x += y;
        System.out.println(x);
        x -= y;
        System.out.println(x);
        x *= y;
        System.out.println(x);
        x /= y;
        System.out.println(x);
        x %= y;
        System.out.println(x);

        x ++;
        System.out.println(x);
    }
}

// Resultados

120
100
2000
100
0
1
